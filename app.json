{
  "expo": {
    "name": "Betweener",
    "owner": "kaakyire9",
    "slug": "betweener",
    "version": "1.0.0",
    "runtimeVersion": {
      "policy": "fingerprint"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_ERROR_RECOVERY",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/7de6cdc4-8616-446f-99de-82764af3a7e6"
    },
    "orientation": "portrait",
    "scheme": "betweenerapp",
    "platforms": [
      "ios",
      "android"
    ],
    "userInterfaceStyle": "automatic",
    "icon": "./assets/images/ios-icon-1024.png",
    "ios": {
      "bundleIdentifier": "com.aduboffour.betweener",
      "icon": "./assets/images/ios-icon-1024.png",
      "associatedDomains": [
        "applinks:getbetweener.com"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "Betweener uses your location to show you nearby matches and improve discovery. You can change this anytime in Settings.",
        "NSPhotoLibraryUsageDescription": "Betweener needs access to your photo library so you can upload profile photos and share moments.",
        "NSPhotoLibraryAddUsageDescription": "Betweener needs permission to save photos you choose to download.",
        "NSCameraUsageDescription": "Betweener needs camera access so you can take profile photos and create moments.",
        "NSMicrophoneUsageDescription": "Betweener uses the microphone for voice messages and audio features in chat.",
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "com.aduboffour.betweener",
            "CFBundleURLSchemes": [
              "betweenerapp"
            ]
          }
        ],
        "LSApplicationQueriesSchemes": [
          "betweenerapp"
        ],
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false
        }
      }
    },
    "android": {
      "package": "com.aduboffour.betweener",
      "googleServicesFile": "./google-services.json",
      "softwareKeyboardLayoutMode": "resize",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/foreground-icon.png",
        "backgroundImage": "./assets/images/background-icon.png"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "betweenerapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "autoVerify": true
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "betweenerapp",
              "host": "auth"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "autoVerify": true
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "getbetweener.com"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "autoVerify": true
        }
      ]
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-router",
        {
          "origin": "betweenerapp://"
        }
      ],
      "expo-font",
      "expo-web-browser",
      "expo-video",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "betweener",
          "organization": "nyansapa-ltd"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7de6cdc4-8616-446f-99de-82764af3a7e6"
      }
    }
  }
}

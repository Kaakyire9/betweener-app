{
  "expo": {
    "name": "betweener-app",
    "slug": "betweener-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "betweenerapp",
    "platforms": [
      "ios",
      "android"
    ],
    "userInterfaceStyle": "automatic",
    "ios": {
      "bundleIdentifier": "com.aduboffour.betweener",
      "associatedDomains": [
        "applinks:getbetweener.com"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "com.aduboffour.betweener",
            "CFBundleURLSchemes": [
              "betweenerapp"
            ]
          }
        ],
        "LSApplicationQueriesSchemes": [
          "betweenerapp"
        ],
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#E6F4FE"
      },
      "package": "com.aduboffour.betweener",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "betweenerapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "autoVerify": true
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "betweenerapp",
              "host": "auth"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "autoVerify": true
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "getbetweener.com"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "autoVerify": true
        }
      ]
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-router",
        {
          "origin": "betweenerapp://"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "fd4f1183-5670-4a5a-be66-573b687825ee"
      }
    }
  }
}

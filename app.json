{
  "expo": {
    "name": "betweener-app",
    "slug": "betweener-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "betweenerapp",
    "platforms": [
      "ios",
      "android"
    ],
    "userInterfaceStyle": "automatic",
    "ios": {
      "bundleIdentifier": "com.aduboffour.betweener",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#E6F4FE"
      },
      "package": "com.aduboffour.betweener",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "betweenerapp",
              "host": "auth",
              "pathPrefix": "/callback"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-router",
        {
          "origin": "betweenerapp://"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "fd4f1183-5670-4a5a-be66-573b687825ee"
      }
    }
  }
}
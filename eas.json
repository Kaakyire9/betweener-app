{
  "cli": {
    "version": ">= 16.26.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "ios": {
        "image": "latest",
        "cache": {
          "disabled": true
        }
      },
      "env": {
        "EXPO_PUBLIC_ENVIRONMENT": "development",
        "EXPO_USE_POD_REPO_UPDATE": "1"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "EXPO_PUBLIC_ENVIRONMENT": "preview",
        "EXPO_USE_POD_REPO_UPDATE": "1"
      },
      "ios": {
        "image": "latest",
        "cache": {
          "disabled": true
        }
      }
    },
    "playInternal": {
      "distribution": "store",
      "autoIncrement": true,
      "channel": "preview",
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_PUBLIC_ENVIRONMENT": "preview",
        "EXPO_USE_POD_REPO_UPDATE": "1"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "env": {
        "EXPO_PUBLIC_ENVIRONMENT": "production",
        "EXPO_USE_POD_REPO_UPDATE": "1"
      },
      "ios": {
        "image": "latest"
      }
    }
  },
  "submit": {
    "playInternal": {},
    "production": {}
  }
}
